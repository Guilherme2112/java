	</welcome-file-list>
	<servlet>
		<servlet-name>produtoServlet</servlet-name>
		<servlet-class>br.com.fiap.servlet.ProdutoServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>produtoServlet</servlet-name>
		<url-pattern>/produtoServlet</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>clienteServlet</servlet-name>
		<servlet-class>br.com.fiap.servlet.ClienteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>clienteServlet</servlet-name>
		<url-pattern>/clienteServlet</url-pattern>
	</servlet-mapping>
</web-app>